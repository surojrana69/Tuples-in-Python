# to count elements in tuples

tuple = (0, 5, 4, 8, 4, 4, 6, 8, 9, 1, 3, 2, 4, 5, 2, 7)
d = int(input("Enter the number from tuple: "))
res = tuple.count(d)
res2 = tuple.index(d)
res3 = len(tuple)
print(f"The count of {d} in tuple is {res}")
print(f"The index of {d} is {res2}")
print("The length of given tuple is",res3)
